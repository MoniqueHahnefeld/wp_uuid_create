{
	"name":"webpixels/contao-wp_uuid_creator",
	"description":"Create UUIDs and repair tl_files if update from prior 3.2 went wrong for contao",
	"keywords":["uuid", "contao", "update", "tool"],
	"type":"contao-module",
	"homepage":"http://www.webpixels.at",
	"license":"LGPL-3.0+",
	"authors":[
		{
			"name":"Johannes Pichler",
			"email":"j.pichler@webpixels.at",
			"homepage":"http://www.webpixels.at",
			"role":"Developer"
		}
	],
	"support":{
		"email":"j.pichler@webpixels.at",
		"issues":"https://github.com/joeherold/wp_uuid_create/issues",
		"irc":"irc://irc.freenode.org/contao.dev",
		"source":"https://github.com/joeherold/wp_uuid_create"
	},
	"require":{
		"php":">=5.3",
		"contao/core": ">=3.2",
                "contao-community-alliance/composer-installer": "*"
		
	},
        "replace":{
		"contao-legacy/wp_uuid_create":"*"
	},
	"extra": {
		"contao": {
			"sources": {
                            "assets": "system/modules/wp_uuid_create/assets",
                            "classes": "system/modules/wp_uuid_create/classes",
                            "config": "system/modules/wp_uuid_create/config",
                            "languages": "system/modules/wp_uuid_create/languages",
                            "templates": "system/modules/wp_uuid_create/templates",
                            "README.md": "system/modules/wp_uuid_create/README.md",
                            "composer.json": "system/modules/wp_uuid_create/composer.json"
                            }
		}
	}
}